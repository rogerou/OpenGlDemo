        </activity>

        <activity
            android:name="TextureActivity"
            android:label="Texture">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name=".DemoActivity" />


        <activity android:name=".CameraActivity" />
    </application>


